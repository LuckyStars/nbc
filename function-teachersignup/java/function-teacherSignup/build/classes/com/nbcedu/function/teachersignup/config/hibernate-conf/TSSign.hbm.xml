<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.nbcedu.function.teachersignup.model.TSSign"
		table="t_teachersignup_sign">
		<id name="id" type="java.lang.String">
			<column name="id" length="32" />
			<generator class="com.nbcedu.common.uuid.UIDGenerator" />
		</id>
		<property name="userName" type="java.lang.String">
			<column name="user_name" length="20" />
		</property>
		<property name="userUid" type="java.lang.String">
			<column name="user_uid" length="32" />
		</property>
		<property name="subjectId" type="java.lang.String">
			<column name="subject_id" length="32" />
		</property>
		<property name="rewardId" type="java.lang.String">
			<column name="reward_id" length="32" />
		</property>
	</class>
</hibernate-mapping>
