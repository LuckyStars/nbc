<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">
	
	<bean id="teachersignupLoader" class="com.nbcedu.function.teachersignup.interceptor.TSLoader"></bean>
	
	<bean id="tsRoleInterceptor" class = "com.nbcedu.function.teachersignup.interceptor.RoleInterceptor">
		<property name="userPrivilegeBiz" ref="tsUserPriBiz"></property>
	</bean>
	
	<bean id="tsEndPubInterceptor" class="com.nbcedu.function.teachersignup.interceptor.EndPubInterceptor">
		<property name="actBiz" ref="tsActBiz"></property>
	</bean>
	
	<bean id="tsUserPriAction" class="com.nbcedu.function.teachersignup.action.TSUserPrivilegeAction" scope="prototype">
		<property name="priBiz" ref="tsUserPriBiz"></property>
	</bean>
	
	<bean id="tsActivityAction" class="com.nbcedu.function.teachersignup.action.TSActivityAction" >
		<property name="actBiz" ref="tsActBiz"></property>
	</bean>
	
	<bean id="tsSignAction" class="com.nbcedu.function.teachersignup.action.TSSignAction">
		<property name="signBiz" ref="tsSignBiz"></property>
		<property name="actBiz" ref="tsActBiz"></property>
		<property name="subBiz" ref="tsSubBiz"></property>
	</bean>
	
</beans>
