<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.nbcedu.function.teachersignup.model.TSUserPrivilege"
		table="t_teachersignup_user_privilege">
		<id name="id" type="java.lang.String">
			<column name="id" length="32" />
			<generator class="com.nbcedu.common.uuid.UIDGenerator" />
		</id>
		<property name="userName" type="java.lang.String">
			<column name="user_name" length="30" />
		</property>
		<property name="userUid" type="java.lang.String">
			<column name="user_uid" length="32" />
		</property>
	</class>
</hibernate-mapping>
