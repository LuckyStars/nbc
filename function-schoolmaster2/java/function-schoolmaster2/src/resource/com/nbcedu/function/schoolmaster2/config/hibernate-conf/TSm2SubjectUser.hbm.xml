<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.nbcedu.function.schoolmaster2.data.model.TSm2SubjectUser" table="t_sm2_subject_user">
        <id name="id" type="java.lang.String">
            <column name="id" length="32" />
            <generator class="com.nbcedu.common.uuid.UIDGenerator" />
        </id>
        <property name="subId" type="java.lang.String">
            <column name="sub_Id" length="32"  />
        </property>
        <property name="userUid" type="java.lang.String">
            <column name="user_uid" length="32" not-null="true" />
        </property>
           <property name="userName" type="java.lang.String">
            <column name="user_Name" length="100"/>
        </property>
        <property name="content" type="java.lang.String">
            <column name="content" length="65535" />
        </property>
    </class>
</hibernate-mapping>
