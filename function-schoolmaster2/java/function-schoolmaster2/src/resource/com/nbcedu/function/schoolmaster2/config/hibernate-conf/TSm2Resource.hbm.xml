<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.nbcedu.function.schoolmaster2.data.model.TSm2Resource" table="t_sm2_resource" >
        <id name="id" type="java.lang.String">
            <column name="id" length="32" />
           <generator class="com.nbcedu.common.uuid.UIDGenerator" />
        </id>
        <property name="createrId" type="java.lang.String">
            <column name="createrId" length="32" />
        </property>
        <property name="createTime" type="java.sql.Timestamp">
            <column name="createTime" length="0" />
        </property>
        <property name="lastUpdateTime" type="java.sql.Timestamp">
            <column name="lastUpdateTime" length="0" />
        </property>
        <property name="fileName" type="java.lang.String">
            <column name="fileName" length="200" />
        </property>
        <property name="filePath" type="java.lang.String">
            <column name="filePath" length="500" />
        </property>
        <property name="progressId" type="java.lang.String">
            <column name="progressId" length="32" />
        </property>
        <property name="type" type="int">
            <column name="type" length="2" />
        </property>
    </class>
</hibernate-mapping>
