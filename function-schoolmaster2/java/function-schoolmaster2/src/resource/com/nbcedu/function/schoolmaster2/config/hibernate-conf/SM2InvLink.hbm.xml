<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.nbcedu.function.schoolmaster2.invitation.model.SM2InvLink" table="t_sm2_inv_link">
		
		<id name="id" column="ID" type="string" length="32">
			<generator class="com.nbcedu.common.uuid.UIDGenerator" />
		</id>
		
		<property name="invId" column="inv_id" type="string" length="32" not-null="true" />
		
		<property name="url" column="url" type="text" not-null="true" />
		
	</class>
</hibernate-mapping>