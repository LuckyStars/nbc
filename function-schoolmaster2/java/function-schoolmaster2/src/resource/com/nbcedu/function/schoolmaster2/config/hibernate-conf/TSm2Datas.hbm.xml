<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.nbcedu.function.schoolmaster2.data.model.TSm2Datas" table="t_sm2_datas" >
        <id name="id" type="java.lang.String">
            <column name="ID" length="32" />
            <generator class="com.nbcedu.common.uuid.UIDGenerator" />
        </id>
        <property name="title" type="java.lang.String">
            <column name="title" length="50" not-null="true" />
        </property>
        <property name="startDate" type="java.sql.Timestamp">
            <column name="start_date" length="0" not-null="true" />
        </property>
        <property name="endDate" type="java.sql.Timestamp">
            <column name="end_date" length="0" not-null="true" />
        </property>
        <property name="createDate" type="java.sql.Timestamp">
            <column name="create_date" length="0" not-null="true" />
        </property>
        <property name="lastUpdate" type="java.sql.Timestamp">
            <column name="last_update" length="0" not-null="true" />
        </property>
        <property name="status" type="java.lang.Integer">
            <column name="status" not-null="true" />
        </property>
        <property name="creatorUid" type="java.lang.String">
            <column name="creator_uid" length="32" not-null="true" />
        </property>
        <property name="content" type="java.lang.String">
            <column name="content" length="65535" not-null="true" />
        </property>
        <property name="matcher" type="java.lang.String">
            <column name="matcher" length="32" not-null="true" />
        </property>
    </class>
</hibernate-mapping>
