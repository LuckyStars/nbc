<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.nbcedu.function.documentflow.model.DocumentSource" table="T_DOCUMENTFLOW_DOCUMENTSOURCE">
		<id name="id" column="PK_T_DOCUMENTFLOW_DOCUMENTSOURCE_ID" type="string" length="32">
			<generator class="com.nbcedu.common.uuid.UIDGenerator" />
		</id>
		<property name="displayName" column="DISPLAYNAME" type="string" length="50" not-null="true" />
		<property name="status" column="STATUS" type="string" length="1" not-null="true" />
	</class>
</hibernate-mapping>