<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.nbcedu.function.documentflow.model.DoucmentUserRole" table="t_documentflow_userRole" >
        <id name="id" type="java.lang.String">
            <column name="id" length="32" />
            <generator class="com.nbcedu.common.uuid.UIDGenerator" />
        </id>
        <property name="pid" type="java.lang.String">
            <column name="pid" length="32" not-null="true" />
        </property>
        <property name="personName" type="java.lang.String">
            <column name="personName" />
        </property>
        <property name="roleName" type="java.lang.String">
            <column name="roleName" />
        </property>
    </class>
</hibernate-mapping>
