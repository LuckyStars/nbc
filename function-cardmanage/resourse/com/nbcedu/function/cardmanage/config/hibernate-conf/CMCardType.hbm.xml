<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.nbcedu.function.cardmanage.model.CMCardType" table="T_CARDMANAGE_CARDTYPE">
		<id name="id" column="PK_CMCARDTYPE_ID" type="string" length="32">
			<generator class="com.nbcedu.common.uuid.UIDGenerator" />
		</id>
		<property name="name" column="CARD_NAME" type="string" not-null="true" />
		<property name="type" column="CARD_TYPE" type="integer" not-null="true" />
	</class>
</hibernate-mapping>