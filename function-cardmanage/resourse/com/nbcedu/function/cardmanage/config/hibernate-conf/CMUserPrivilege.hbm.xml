<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.nbcedu.function.cardmanage.model.CMUserPrivilege" table="T_CARDMANAGE_USERPRIVILEGE">
		<id name="id" column="PK_USERPRIVILEGE_ID" type="string" length="32">
			<generator class="com.nbcedu.common.uuid.UIDGenerator" />
		</id>
		<property name="uid" column="UID" type="string" length="32" not-null="true" />
		<property name="userName" column="USERNAME" type="string" length="100" />
	</class>
</hibernate-mapping>